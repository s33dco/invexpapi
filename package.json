{
	"name": "invexpapi",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "https://github.com/s33dco/invexpapi.git",
	"author": "s33dco <code@s33d.co>",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": "12.4.0",
		"npm": "6.9.0"
	},
	"scripts": {
		"clientinstall": "cd client && npm install",
		"server": "nodemon server/server.js",
		"indev": "concurrently \"cd client && npm run dev\" \"npm run server\"",
		"test": "jest --watchAll --runInBand",
		"analyze": "cd client && build:analyze",
		"inprod": "concurrently \"cross-env NODE_ENV=production && cd client && npm run build\" \" cross-env NODE_ENV=production npm run start\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && cd client && npm run build",
		"start": "node server/server.js"
	},
	"dependencies": {
		"@hapi/joi": "^15.1.1",
		"bcryptjs": "^2.4.3",
		"compression": "^1.7.4",
		"config": "^3.2.2",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"helmet": "^3.21.2",
		"joi-objectid": "^3.0.1",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.24.0",
		"mongodb": "^3.3.3",
		"mongoose": "^5.7.7",
		"morgan": "^1.9.1",
		"multer": "^1.4.2",
		"nodemailer": "^6.3.1",
		"nodemailer-sendgrid-transport": "^0.2.0",
		"omit-empty": "^1.0.0",
		"redux": "^4.0.4",
		"winston": "^3.2.1",
		"winston-daily-rotate-file": "^4.2.1",
		"winston-mongodb": "^5.0.0"
	},
	"devDependencies": {
		"concurrently": "^5.0.0",
		"cross-env": "^6.0.3",
		"faker": "^4.1.0",
		"nodemon": "^1.19.4",
		"supertest": "^4.0.2"
	}
}
