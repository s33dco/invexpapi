	"license": "MIT",
	"private": true,
	"engines": {
		"node": "12.4.0"
	},
	"config": {
		"webpack_args": "webpack --config build-utils/webpack.config.js --display-error-details"
	},
	"scripts": {
		"dev:prebuild": "rimraf dev",
		"dev:build": "cross-env NODE_ENV=development $npm_package_config_webpack_args --env.env=dev",
		"prebuild": "rimraf build",
		"build": "cross-env NODE_ENV=production $npm_package_config_webpack_args --env.env=prod",
		"dev:start": "nodemon server/server.js",
		"start": "NODE_ENV=production node server/server.js",
		"dev": "concurrently \"yarn dev:start\" \"webpack-dev-server --env.env=dev\"",
		"runinprod": "concurrently \"cross-env NODE_ENV=production yarn build\" \" cross-env NODE_ENV=production yarn start\"",
		"runindev": "concurrently \"cross-env NODE_ENV=development yarn dev:build\" \" cross-env NODE_ENV=development yarn dev:start\"",
		"build:analyze": "npm run build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
		"test": "jest --watchAll --runInBand",
		"heroku-postbuild": "yarn run build"
	},
